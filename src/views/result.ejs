<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head') %> 
<body>
    <%- include('./partials/header') %>
    <div class="container">
        <div class="row">
            <% gifs.forEach(gif => { %>
                <div class="col-12 col-md-6 col-lg-3">
                    <div class="card my-2" >
                        <img src="<%= gif.images.original.url %>" class="card-img-top" alt="...">
                        <div class="card-body">
                          <p class="card-text"><%= gif.title %> </p>
                        </div>
                      </div>
                </div>
            <% }) %>
        </div>
    </div>
   
</body>
</html>